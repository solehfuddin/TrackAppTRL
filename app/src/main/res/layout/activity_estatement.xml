                android:layout_height="wrap_content"
                android:text="TextView"
                android:visibility="gone"
                app:layout_constraintLeft_toRightOf="@+id/txtMonAwal"
                app:layout_constraintTop_toBottomOf="@+id/form_estatement_linearselect" />

            <TextView
                android:id="@+id/txtMonAkhir"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:visibility="gone"
                app:layout_constraintLeft_toRightOf="@+id/txtYearAwal"
                app:layout_constraintTop_toBottomOf="@+id/form_estatement_linearselect" />

            <TextView
                android:id="@+id/txtYearAkhir"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:visibility="gone"
                app:layout_constraintLeft_toRightOf="@+id/txtMonAkhir"
                app:layout_constraintTop_toBottomOf="@+id/form_estatement_linearselect" />

            <com.dueeeke.tablayout.SlidingTabLayout
                android:id="@+id/form_estatement_slidinglayout"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@color/dot_dark_screen2"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@+id/form_estatement_rplfilter"
                app:layout_constraintTop_toBottomOf="@+id/cardView8"
                android:visibility="gone"
                tl:tl_divider_color="@color/dot_dark_screen2"
                tl:tl_divider_padding="13dp"
                tl:tl_divider_width="1dp"
                tl:tl_indicator_color="@color/neutral"
                tl:tl_indicator_height="2dp"
                tl:tl_indicator_width_equal_title="true"
                tl:tl_tab_padding="12dp"
                tl:tl_tab_space_equal="false"
                tl:tl_textSelectColor="@color/neutral"
                tl:tl_textUnselectedColor="@color/gray"
                tl:tl_underline_color="@color/dot_dark_screen2"
                tl:tl_underline_height="1dp" />

            <com.andexert.library.RippleView
                android:id="@+id/form_estatement_rplexport"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_marginLeft="0dp"
                android:background="@color/dot_dark_screen2"
                app:layout_constraintBottom_toTopOf="@+id/form_estatement_viewpager"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/form_estatement_lineardate">

                <ImageView
                    android:id="@+id/form_estatement_btnexport"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_centerInParent="true"
                    android:src="@drawable/ic_export_white" />

            </com.andexert.library.RippleView>

            <LinearLayout
                android:id="@+id/linearLayout5"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:background="@color/dot_dark_screen2"
                android:orientation="horizontal"
                app:layout_constraintRight_toLeftOf="@+id/form_estatement_rplfilter"
                app:layout_constraintTop_toBottomOf="@+id/form_estatement_lineardate"
                app:layout_constraintLeft_toRightOf="@+id/form_estatement_rplexport"
                android:layout_marginLeft="0dp">

                <com.raizlabs.universalfontcomponents.widget.UniversalFontTextView
                    android:id="@+id/form_estatement_txtTitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="24 Nov 2020"
                    android:layout_gravity="center_vertical"
                    android:textColor="@color/neutral"
                    android:textSize="16sp"
                    app:customFont="UbuntuMedium.ttf" />
            </LinearLayout>

            <com.andexert.library.RippleView
                android:layout_width="50dp"
                android:layout_height="40dp"
                app:layout_constraintTop_toBottomOf="@+id/form_estatement_lineardate"
                android:background="@color/dot_dark_screen2"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/form_estatement_viewpager"
                android:id="@+id/form_estatement_rplfilter">

                <ImageView
